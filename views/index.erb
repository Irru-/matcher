<div class="form_div_50">
	<form method='POST' action = '/uploadform'>

	<table>
	
		<tr>
			<td>Locatie</td>
			<td><input type='text' name='match[l1]'></td>
			<td></td>
		</tr>
	
		<tr>
			<td>Skill</td>
			<td><input type='text' name='match[sk]'></td>
		</tr>
	
		<tr>
			<td>Maximale afstand</td>
			<td><input type='text' id='afstand' onkeypress="return isNumberKeyLoc(event)" name='match[di]'></td>
			<td><input type='text' id='locatie' onkeypress="return isNumberKey(event)" name='match[lo]'></td>
		</tr>
	
		<tr>
			<td>Opleiding</td>
			<td>	
				<select name = 'match[edu]'>
			  		<option value="5">MBO</option>
			  		<option value="10">HBO</option>
			  		<option value="15">WO</option>
				</select>
			</td>
			<td><input type='text' id='edu' onkeypress="return isNumberKey(event)" name='match[ed]'></td>
		</tr>
	
		<tr>
			<td>Dienstverband</td>
			<td>
				<select name = 'match[dvb]'>
					<option value="full">Full time</option>
					<option value="part">Part time</option>
					<option value="detachering">Detachering/Interim</option>
					<option value="stage">Stage</option>
					<option value="bijbaan">Bijbaan</option>
					<option value="freelance">Freelance</option>
				</select>
			</td>
		</tr>
	
	</table>
	
		<input type='submit' value='Match me!'>
	</form>

</div>

<div class="form_div_50">

	<form method="post" enctype='multipart/form-data')>
		<input type='file' name='myfile'> <br />
		Maximale afstand <input type='text' id='afstand' onkeypress="return isNumberKeyLoc(event)" name='match[di]'> <br/>
		Dienstverband
		<select name = 'match[dvb]'>
			<option value="full">Full time</option>
			<option value="part">Part time</option>
			<option value="detachering">Detachering/Interim</option>
			<option value="stage">Stage</option>
			<option value="bijbaan">Bijbaan</option>
			<option value="freelance">Freelance</option>
		</select> <br />
		Locatiescore<input type='text' id='locatie' onkeypress="return isNumberKey(event)" name='match[lo]'> <br />
		Skillscore<input type='text' id='skill' onkeypress="return isNumberKey(event)" name='match[sk]'> <br />
		Educationscore<input type='text' id='edu' onkeypress="return isNumberKey(event)" name='match[ed]'> <br />
		<input type='submit' value='Upload!' onclick="return notEmpty('afstand','locatie','skill','edu','Vul alle velden in aub.')" />
	</form>
</div>